{
  "expo": {
    "name": "Divin8",
    "slug": "divin8-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "scheme": "divin8",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*",
      "!**/*.backup.*",
      "!**/*-backup.*",
      "!**/node_modules/**",
      "!**/.git/**",
      "!**/android/**",
      "!**/ios/**"
    ],
    "ios": {
      "bundleIdentifier": "com.divin8.app",
      "buildNumber": "1",
      "supportsTablet": true,
      "deploymentTarget": "15.1",
      "infoPlist": {
        "CFBundleDisplayName": "Divin8",
        "CFBundleAllowMixedLocalizations": true,
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "divin8"
            ]
          }
        ],
        "ITSAppUsesNonExemptEncryption": false
      },
      "associatedDomains": [
        "applinks:share.divin8.com"
      ]
    },
    "android": {
      "package": "com.divin8.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "permissions": [
        "INTERNET"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "share.divin8.com",
              "pathPrefix": "/r"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ],
      "./plugins/with-disable-lint-extratranslation.js"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "EXPO_PUBLIC_EXPO_PROJECT_FULL_NAME": "@irowen/divin8-app",
      "eas": {
        "projectId": "b3361d2b-b33f-4918-8442-cd6b2bdb7c9a"
      }
    },
    "owner": "irowen",
    "locales": {
      "en": {
        "displayName": "Divin8"
      },
      "zh-TW": {
        "displayName": "Divin8"
      }
    }
  }
}
