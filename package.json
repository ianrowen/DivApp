{
  "name": "divin8-app",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "sync-translations": "ts-node src/scripts/syncTranslations.ts",
    "sync-translations:dry": "ts-node src/scripts/syncTranslations.ts --dry-run",
    "sync-translations:verbose": "ts-node src/scripts/syncTranslations.ts --verbose",
    "optimize-image": "node scripts/optimize-problematic-image.js",
    "convert-to-webp": "node scripts/convert-to-webp.js",
    "optimize-all-images": "node scripts/optimize-all-images.js",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "build:android:preview": "eas build --platform android --profile preview",
    "build:ios:preview": "eas build --platform ios --profile preview",
    "build:ios:simulator": "eas build --platform ios --profile simulator",
    "build:android:production": "eas build --platform android --profile production",
    "build:ios:production": "eas build --platform ios --profile production",
    "build:all:production": "eas build --platform all --profile production",
    "submit:ios": "eas submit --platform ios --profile production",
    "submit:android": "eas submit --platform android --profile production",
    "submit:all": "eas submit --platform all --profile production",
    "release:ios": "eas build --platform ios --profile production --auto-submit",
    "release:android": "eas build --platform android --profile production --auto-submit",
    "release:all": "eas build --platform all --profile production --auto-submit",
    "version:bump:patch": "npm version patch --no-git-tag-version && node scripts/update-app-version.js",
    "version:bump:minor": "npm version minor --no-git-tag-version && node scripts/update-app-version.js",
    "version:bump:major": "npm version major --no-git-tag-version && node scripts/update-app-version.js"
  },
  "dependencies": {
    "@expo-google-fonts/cinzel": "^0.4.2",
    "@expo-google-fonts/lato": "^0.4.1",
    "@google/generative-ai": "^0.24.1",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/netinfo": "^11.4.1",
    "@supabase/supabase-js": "^2.39.0",
    "astronomy-engine": "^2.1.19",
    "axios": "^1.13.2",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "expo": "~54.0.25",
    "expo-auth-session": "~7.0.8",
    "expo-build-properties": "~1.0.9",
    "expo-constants": "~18.0.10",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.15",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.9",
    "i18n-js": "^4.5.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "4.1.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@types/node": "^24.10.1",
    "@types/react": "~19.1.10",
    "babel-preset-expo": "^54.0.7",
    "sharp": "^0.34.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "private": true
}
